{
  "title": "Wolf Curses",
  "copyright": "Maxwolf",
  "description": "Console control library",
  "language": "en-US",
  "version": "2.1.0.9",
  "buildOptions": {
    "optimize": true,
    "xmlDoc": true
  },
  "dependencies": {
    "Cake.Core": "0.16.2",
    "Microsoft.Extensions.DependencyModel": "1.0.0",
    "NETStandard.Library": "1.6.0",
    "System.Linq": "4.1.0",
    "System.Reflection": "4.1.0",
    "System.Reflection.TypeExtensions": "4.1.0",
    "System.Threading.Thread": "4.0.0"
  },
  "frameworks": {
    "netstandard1.6": {
      "imports": "dnxcore50"
    }
  },
  "tools": {
    "dotnet-bump": "1.0.1"
  },
  "scripts": {
    "postcompile": [
      "dotnet bump revision -t Debug -c %compile:Configuration%",
      "dotnet bump revision -t Release -c %compile:Configuration%"
    ]
  },
  "configurations": {
    "Release": {},
    "Debug": {},
    "Jenkins": {}
  },
  "runtimes": {
    "win7-x64": {},
    "osx.10.10-x64": {},
    "ubuntu.14.04-x64": {}
  }
